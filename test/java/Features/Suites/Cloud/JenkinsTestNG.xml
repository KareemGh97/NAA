<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.1.dtd">

<suite name="UI Automation" share-thread-pool-for-data-providers="true" data-provider-thread-count="8">

    <listeners>
        <listener class-name="Utilities.TestListeners.retest.AnnotationTransformer"/>
    </listeners>
    <!--    for retry failed TC uncomment above â¬†  -->

    <parameter name="deployment" value="Cloud"/>
    <parameter name="port" value=""/>
    <parameter name="environment" value="${environment}"/>
    <parameter name="language" value="en"/>
    <parameter name="url" value="http://hub.browserstack.com/wd/hub"/>
    <parameter name="locale" value="SA"/>
    <parameter name="udid" value="96C206D0-E7AC-4651-AC2C-73BA8343B5FF"/>

    <!--    common parameter    -->

    <test name="${executionCycle} ${platformName} ${featureNameTag}">
        <parameter name="executionCycle" value="${executionCycle}"/>
        <parameter name="platformName" value="${platformName}"/>      <!--Android,iOS-->
        <parameter name="automationName" value="${automationName}"/><!--UIAutomator2,XCUITest-->
        <parameter name="featureNameTag" value="${featureNameTag}"/>

        <classes>
            <class name="TestRunners.JenkinsRunnerTest"/>
        </classes>
    </test>

</suite>